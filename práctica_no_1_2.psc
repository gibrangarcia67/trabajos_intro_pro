Algoritmo práctica_no_1_2
	
	a <- 0
	b <- 0
	
	Escribir "Digita el primer valor"
	Leer a
	
	Escribir  "Digita el segundo valor"
	Leer b
	
	Escribir "Valor del primer numero: ",a,"."
	
	Escribir "Valor del segundo numero: ",b,"."
	
	ax <- a
	a <- b
	b <- ax
	
	Escribir "Valor de A, intercambiado A a B: ",a,"."
	Escribir "Valor de B, intercambiado de B a A: ",b,"."
	
FinAlgoritmo
