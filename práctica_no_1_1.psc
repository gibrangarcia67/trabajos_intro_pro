Algoritmo práctica_no_1_1
	
	a <- 0
	b <- 0
	
	Escribir "Digita un numero"
	Leer  a
	
	Escribir "Digita un numero"
	Leer b
	
	si a > b Entonces
		
		Escribir "El primer numero dado (",a,") es mayor al segundo (",b,")."
	
	FinSi
	
	si b > a Entonces
		
			Escribir "El segundo numero dado (",b,") es mayor al primero (",a,")."
		
	FinSi
	
	si a == b Entonces
		
		Escribir "Los valores dados son iguales"
		
	FinSi
	
FinAlgoritmo
