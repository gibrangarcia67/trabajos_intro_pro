Algoritmo práctica_no_3_1
	a <- 0
	b <- 0
	
	Escribir "Numero de hombres"
	Leer a
	
	Escribir "Numero de mujeres"
	Leer b
	
	t <- a + b
	
	Escribir "Total de alumnos en el curso ",t
	
	ph <- a / t * 100
	
	Escribir "Porcentaje de Hombres en el curso: %",ph,"."
	
	pm <- b / t *100
	
	Escribir "Porcentaje de mujeres: %",pm, "."
	
	
	
	
FinAlgoritmo
